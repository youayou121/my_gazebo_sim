<?xml version="1.0"?>
<launch>
    <node name="map_server" pkg="map_server" type="map_server"
        args="$(find test_mycar)/maps/my_room_wall.yaml">
        <param name="frame_id" value="map" />
    </node>
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find test_mycar)/param/costmap/common.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find test_mycar)/param/costmap/common.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find test_mycar)/param/costmap/local.yaml" command="load" />
        <rosparam file="$(find test_mycar)/param/costmap/global.yaml" command="load" />
        <rosparam file="$(find test_mycar)/param/tracer/teb.yaml" command="load" />
        <!-- <rosparam file="$(find test_mycar)/param/tracer/dwa.yaml" command="load" /> -->
        <rosparam file="$(find test_mycar)/param/tracer/move_base.yaml" command="load" />
    </node>
    <include file="$(find test_mycar)/launch/amcl.launch"/>
     <node name="rviz" pkg="rviz" type="rviz"  output="log" args = "-d $(find test_mycar)/config/tracer/tracer.rviz"/>
</launch>